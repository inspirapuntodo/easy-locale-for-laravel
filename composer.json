{
    "name": "inspirapuntodo/easy-locale-for-laravel",
    "description": "Easily manage your localization in your laravel app",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "InspiraPuntoDo\\EasyLocale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Cesar Mendez",
            "email": "cesar@camv.dev"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/pint": "^1.13"
    },
    "require-dev": {
        "laravel/framework": "^10.31",
        "nunomaduro/larastan": "^2.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "InspiraPuntoDo\\EasyLocale\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
